{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "curly": "error",
        "no-console": false,
        "no-var-requires": false,
        "trailing-comma": [ false ],
        "variable-name": [
            "allow-pascal-case",
            "ban-keywords",
            "allow-leading-underscore"
        ]
        
    },
    "rulesDirectory": []
}
